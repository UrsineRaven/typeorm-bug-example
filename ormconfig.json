{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "entity/**/*.js"
   ],
   "migrations": [
      "migration/**/*.js"
   ],
   "subscribers": [
      "subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "entity",
      "migrationsDir": "migration",
      "subscribersDir": "subscriber"
   }
}